{
  "source": "./src/assets/",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "lint": { "enable": true },
        "coverage": {"enable": true},
        "accessor": {"access": ["public", "protected", "private"], "autoPrivate": true},
        "undocumentIdentifier": {"enable": true},
        "unexportedIdentifier": {"enable": false},
        "typeInference": {"enable": true},
        "brand": {
          "logo": "./cat.png",
          "title": "peer.cat Library",
          "description": "A peer2peer file sharing service and library",
          "repository": "https://github.com/posixpascal/peer.cat",
          "site": "https://peer.cat",
          "author": "https://pascalraszyk.de",
          "image": "./cat.png"
        }
      }
    }
  ]
}
